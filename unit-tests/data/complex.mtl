<?xml version="1.0" encoding="UTF-8" ?>

<material>
	<shaderProg>
		<customMsSProg>
			<defines>
				<define>DIFFUSE_MAPPING</define>
				<define>NORMAL_MAPPING</define>
				<define>ENVIRONMENT_MAPPING</define>
			</defines>
		</customMsSProg>
	</shaderProg>

	<userDefinedVars>
		<userDefinedVar>
			<name>diffuseMap</name>
			<value>
				<texture>meshes/horse/horse.diff.png</texture>
			</value>
		</userDefinedVar>
	
		<userDefinedVar>
			<name>normalMap</name>
			<value>
				<texture>meshes/horse/horse.norm.png</texture>
			</value>
		</userDefinedVar>
	
		<userDefinedVar>
			<name>environmentMap</name>
			<value>
				<texture>textures/env/env_map_3.png</texture>
			</value>
		</userDefinedVar>
	
		<userDefinedVar>
			<name>specularCol</name>
			<value>
				<vec3><x>1.0</x><y>2.0</y><z>-0.8</z></vec3>
			</value>
		</userDefinedVar>
	
		<userDefinedVar>
			<name>diffuseCol</name>
			<value>
				<vec3><x>1.0</x><y>1.0</y><z>1.0</z></vec3>
			</value>
		</userDefinedVar>
	
		<userDefinedVar>
			<name>shininess</name>
			<value>
				<float>90.0</float>
			</value>
		</userDefinedVar>
	</userDefinedVars>
</material>

